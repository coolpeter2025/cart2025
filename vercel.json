{
  "images": {
    "domains": ["vercel.app"],
    "remotePatterns": [
      {
        "protocol": "https",
        "hostname": "**.vercel.app"
      }
    ],
    "unoptimized": true
  },
  "build": {
    "env": {
      "NEXT_PUBLIC_VERCEL_URL": "cart2025.vercel.app"
    }
  },
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "/public/images/",
      "continue": true
    },
    {
      "handle": "filesystem"
    }
  ]
}
